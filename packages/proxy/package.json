{
  "name": "@pag0/proxy",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run --env-file ../../.env.local --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "bun run dist/index.js",
    "test": "bun test",
    "lint": "tsc --noEmit",
    "db:migrate": "bun run --env-file ../../.env.local src/db/migrate.ts",
    "db:seed": "bun run --env-file ../../.env.local src/db/seed.ts"
  },
  "dependencies": {
    "hono": "^4.7.0",
    "@hono/node-server": "^1.13.0",
    "@x402/fetch": "^2.3.0",
    "ioredis": "^5.4.0",
    "postgres": "^3.4.0",
    "ethers": "^6.13.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  }
}
