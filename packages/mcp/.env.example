# Pag0 Proxy API
PAG0_API_URL=https://pag0-proxy.fly.dev
PAG0_API_KEY=pag0_test_your_api_key_here

# Wallet mode: "local" (ethers.Wallet) or "cdp" (Coinbase Server Wallet)
WALLET_MODE=local

# Agent Wallet — Local mode (required if WALLET_MODE=local)
WALLET_PRIVATE_KEY=0x_your_private_key_here
NETWORK=base-sepolia

# CDP Wallet — Coinbase Server Wallet (required if WALLET_MODE=cdp)
CDP_API_KEY_ID=
CDP_API_KEY_SECRET=
CDP_WALLET_SECRET=

# External API keys (optional, auto-injected as Authorization headers)
OPENAI_API_KEY=sk-your_openai_key_here
ANTHROPIC_API_KEY=sk-ant-your_anthropic_key_here
